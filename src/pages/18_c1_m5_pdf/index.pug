extends ../../templates/page/template.page-base-adaptativo.pug

block styles
    link(rel="stylesheet", href="style.css")

block body
    .container
        .containerPDF
            .embed-container
                canvas#the-canvas(pdfPath="page18.pdf")
            .close-page(page="04_c1", materialComplete=5)
                +image('closeBtn.svg')
            .loader#loader
                .lds-eclipse(style='width:100%;height:100%')
                    .spinner
                    .spinner.reverse
                    
   

block scripts
    <script src="//mozilla.github.io/pdf.js/build/pdf.js"></script>
    +scriptVendor('pdfCanvas.js')
    script(src="script.js")