extends ../../templates/page/template.page-base-adaptativo.pug

block styles
    link(rel="stylesheet", href="style.css")

block body
    .container
        .containerPDF
            .embed-container.embed-container-frame
                iframe#isPDF(src="../../assets/pdf/page37.pdf#toolbar=0&navpanes=0&scrollbar=0&view=FitH" frameborder="0")
                //- canvas#the-canvas(pdfPath="page37.pdf")
            .close-page(page="22_c2", materialComplete=3)
                +image('closeBtn.svg')
            .loader.loaderPDF#loader
                .lds-eclipse(style='width:100%;height:100%')
                    .spinner
                    .spinner.reverse
                    
   

block scripts
    //- <script src="//mozilla.github.io/pdf.js/build/pdf.js"></script>
    +scriptVendor('pdfCanvas.js')
    script(src="script.js")